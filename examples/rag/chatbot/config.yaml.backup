cache:
  cleanup_interval_seconds: 300
  default_ttl_seconds: 3600
  enabled: true
  max_size: 10000
  provider: local
chatbot:
  generation:
    max_output_tokens: 1024
    model_name: meta-llama/llama-4-scout-17b-16e-instruct
    provider: groq
    temperature: 0.2
    template: rag_prompt.jinja2
    templates_dir: config/prompts/chatbot
    top_p: 0.95
  host: 0.0.0.0
  memory:
    postgres:
      connection_string: postgresql://rushabhsmacbook@localhost:5432/langgraph_test_db
    store_type: postgres
    type: langgraph_checkpoint
  port: 8001
  reranking:
    enabled: true
    keyword_weight: 0.3
    length_weight: 0.2
    min_score_threshold: 0.0
    position_weight: 0.1
    score_threshold: 0.5
    tfidf_weight: 0.4
    top_k: 3
    type: custom
  retrieval:
    similarity_threshold: 0.7
    top_k: 5
    vector_store:
      dimension: 768
      index_method: hnsw
      index_path: ./data/indices/faiss.index
      metadata_path: ./data/indices/faiss_metadata.json
      type: faiss
  workers: 4
embedding:
  config:
    batch_size: 32
    device: cpu
    model: all-mpnet-base-v2
    normalize_embeddings: true
  provider: sentence_transformer
security:
  api_keys:
  - key: ${API_KEY}
    name: default
    roles:
    - user
  - key: ${ADMIN_API_KEY}
    name: admin
    roles:
    - user
    - admin
  rate_limiting:
    enabled: true
    period_seconds: 60
    provider: local
    requests: 100
system:
  environment: development
  log_level: INFO
  request_id_header: X-Request-ID
vision:
  config:
    model_name: meta-llama/llama-4-scout-17b-16e-instruct
  provider: groq
